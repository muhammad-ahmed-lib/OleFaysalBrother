<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgVuColor"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.OleOwnerEarningFragment">

    <androidx.cardview.widget.CardView
        android:id="@+id/card_vu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_45sdpp"
        android:layout_marginTop="@dimen/_15sdpp"
        android:layout_marginStart="@dimen/_15sdpp"
        android:layout_marginEnd="@dimen/_15sdpp"
        app:cardBackgroundColor="@color/whiteColor"
        app:cardCornerRadius="@dimen/_8sdpp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/rel_menu"
                android:layout_width="@dimen/_40sdpp"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/_5sdpp">

                <ImageView
                    android:id="@+id/img_vu_menu"
                    android:layout_width="@dimen/_25sdpp"
                    android:layout_height="@dimen/_25sdpp"
                    android:layout_centerInParent="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/p_menu_ic" />

            </RelativeLayout>

            <View
                android:id="@+id/vu1"
                android:layout_width="@dimen/_1sdpp"
                android:layout_height="match_parent"
                android:background="#3B707070"
                android:layout_toEndOf="@id/rel_menu"
                android:layout_marginTop="@dimen/_5sdpp"
                android:layout_marginBottom="@dimen/_5sdpp"/>

            <TextView
                android:id="@+id/tv_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/earnings"
                android:textColor="@color/darkTextColor"
                android:layout_toEndOf="@id/vu1"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@id/linear1"
                android:layout_marginStart="@dimen/_10sdpp"
                android:fontFamily="sans-serif-medium"/>

            <LinearLayout
                android:id="@+id/linear1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_alignParentEnd="true">

                <RelativeLayout
                    android:id="@+id/rel_filter"
                    android:layout_width="@dimen/_35sdpp"
                    android:layout_height="match_parent"
                    android:layout_gravity="end">

                    <ImageView
                        android:id="@+id/img_vu_filter"
                        android:layout_width="@dimen/_25sdpp"
                        android:layout_height="@dimen/_25sdpp"
                        android:layout_centerInParent="true"
                        android:scaleType="centerInside"
                        android:src="@drawable/p_filter_ic" />

                </RelativeLayout>

                <View
                    android:layout_width="@dimen/_1sdpp"
                    android:layout_height="match_parent"
                    android:background="#3B707070"
                    android:layout_marginTop="@dimen/_5sdpp"
                    android:layout_marginBottom="@dimen/_5sdpp"/>

                <RelativeLayout
                    android:id="@+id/rel_notif"
                    android:layout_width="@dimen/_35sdpp"
                    android:layout_height="match_parent"
                    android:layout_gravity="end">

                    <ImageView
                        android:id="@+id/img_vu_notif"
                        android:layout_width="@dimen/_25sdpp"
                        android:layout_height="@dimen/_25sdpp"
                        android:layout_centerInParent="true"
                        android:src="@drawable/p_notification_ic"
                        android:scaleType="centerInside"/>

                    <com.nex3z.notificationbadge.NotificationBadge
                        android:id="@+id/toolbar_badge"
                        android:layout_width="@dimen/_22sdpp"
                        android:layout_height="@dimen/_22sdpp"
                        app:nbMaxTextLength="3"
                        android:layout_marginTop="@dimen/_3sdpp"
                        android:layout_marginEnd="@dimen/_3sdpp"
                        android:background="@drawable/badge_bg_with_shadow"
                        android:textSize="@dimen/_8sdpp"
                        android:textAlignment="center"
                        android:layout_alignParentEnd="true"
                        android:clickable="false"
                        android:visibility="visible"
                        android:textColor="@color/whiteColor" />

                </RelativeLayout>

            </LinearLayout>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/pull_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/card_vu"
        android:layout_marginTop="@dimen/_10sdpp">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusableInTouchMode="true"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:id="@+id/top"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdpp"
                    android:layout_marginTop="@dimen/_5sdpp"
                    android:layout_marginEnd="@dimen/_15sdpp"
                    app:cardCornerRadius="@dimen/_8sdpp"
                    app:cardElevation="0dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:id="@+id/rel"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/blueColorNew">

                            <LinearLayout
                                android:id="@+id/ln"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_5sdpp"
                                android:layout_toStartOf="@+id/btn_export"
                                android:orientation="vertical"
                                android:padding="@dimen/_10sdpp">

                                <TextView
                                    android:id="@+id/tv_date"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="Today's earning"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                    android:textColor="#A8FFFFFF" />

                                <TextView
                                    android:id="@+id/tv_total_earning"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_3sdpp"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="0 AED"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                    android:textColor="@color/whiteColor" />

                                <TextView
                                    android:id="@+id/tv_new_customers"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sdpp"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="New customers: 0"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="#A8FFFFFF" />

                            </LinearLayout>

                            <com.google.android.material.card.MaterialCardView
                                android:id="@+id/btn_export"
                                android:layout_width="@dimen/_70sdpp"
                                android:layout_height="@dimen/_30sdpp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/_10sdpp"
                                android:layout_marginEnd="@dimen/_10sdpp"
                                android:theme="@style/Theme.MaterialComponents.Light"
                                app:cardBackgroundColor="#2E000000"
                                app:cardCornerRadius="@dimen/_5sdpp"
                                app:cardElevation="0dp"
                                app:strokeColor="@color/whiteColor"
                                app:strokeWidth="@dimen/_1sdpp">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:fontFamily="sans-serif"
                                    android:text="@string/export"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="@color/whiteColor" />

                            </com.google.android.material.card.MaterialCardView>

                            <ImageView
                                android:layout_width="@dimen/_80sdpp"
                                android:layout_height="@dimen/_80sdpp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/_minus10sdpp"
                                android:layout_marginEnd="@dimen/_minus10sdpp"
                                android:scaleType="centerInside"
                                android:src="@drawable/wallet_ball" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/ln"
                                android:layout_marginStart="@dimen/_10sdpp"
                                android:layout_marginEnd="@dimen/_10sdpp"
                                android:layout_marginBottom="@dimen/_10sdpp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <androidx.cardview.widget.CardView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="sans-serif"
                                                android:text="@string/cash"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_cash"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                    </androidx.cardview.widget.CardView>

                                    <View
                                        android:layout_width="@dimen/_7sdpp"
                                        android:layout_height="match_parent" />

                                    <androidx.cardview.widget.CardView
                                        android:id="@+id/online_payment_vu"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="sans-serif"
                                                android:text="@string/online"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_online"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                        <ImageView
                                            android:layout_width="@dimen/_10sdpp"
                                            android:layout_height="@dimen/_10sdpp"
                                            android:layout_gravity="center_vertical|end"
                                            android:layout_marginEnd="@dimen/_5sdpp"
                                            android:scaleType="centerInside"
                                            android:src="@drawable/right_arrow_white" />

                                    </androidx.cardview.widget.CardView>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_7sdpp"
                                    android:orientation="horizontal">

                                    <androidx.cardview.widget.CardView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="sans-serif"
                                                android:text="@string/device"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_pos"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                    </androidx.cardview.widget.CardView>

                                    <View
                                        android:layout_width="@dimen/_7sdpp"
                                        android:layout_height="match_parent" />

                                    <androidx.cardview.widget.CardView
                                        android:id="@+id/late_payment_vu"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="sans-serif"
                                                android:text="@string/late_payment"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_late_payment"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                        <ImageView
                                            android:layout_width="@dimen/_10sdpp"
                                            android:layout_height="@dimen/_10sdpp"
                                            android:layout_gravity="center_vertical|end"
                                            android:layout_marginEnd="@dimen/_5sdpp"
                                            android:scaleType="centerInside"
                                            android:src="@drawable/right_arrow_white" />

                                    </androidx.cardview.widget.CardView>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_7sdpp"
                                    android:orientation="horizontal">

                                    <androidx.cardview.widget.CardView
                                        android:id="@+id/match_fee_vu"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:ellipsize="end"
                                                android:fontFamily="sans-serif"
                                                android:lines="1"
                                                android:text="@string/match_fee"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_match_fee"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                    </androidx.cardview.widget.CardView>

                                    <View
                                        android:layout_width="@dimen/_7sdpp"
                                        android:layout_height="match_parent" />

                                    <androidx.cardview.widget.CardView
                                        android:id="@+id/paid_balance_vu"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:ellipsize="end"
                                                android:fontFamily="sans-serif"
                                                android:lines="1"
                                                android:text="@string/paid_balance"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_paid_balance"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                        <ImageView
                                            android:layout_width="@dimen/_10sdpp"
                                            android:layout_height="@dimen/_10sdpp"
                                            android:layout_gravity="center_vertical|end"
                                            android:layout_marginEnd="@dimen/_5sdpp"
                                            android:scaleType="centerInside"
                                            android:src="@drawable/right_arrow_white" />

                                    </androidx.cardview.widget.CardView>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_7sdpp"
                                    android:orientation="horizontal">

                                    <androidx.cardview.widget.CardView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="sans-serif"
                                                android:text="@string/discount"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_discount"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                    </androidx.cardview.widget.CardView>

                                    <View
                                        android:layout_width="@dimen/_7sdpp"
                                        android:layout_height="match_parent" />

                                    <androidx.cardview.widget.CardView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        app:cardBackgroundColor="#1A000000"
                                        app:cardCornerRadius="@dimen/_5sdpp"
                                        app:cardElevation="0dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="center_horizontal"
                                            android:minWidth="@dimen/_70sdpp"
                                            android:orientation="vertical"
                                            android:padding="@dimen/_5sdpp">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:fontFamily="sans-serif"
                                                android:text="@string/expense"
                                                android:textColor="#B0FFFFFF" />

                                            <TextView
                                                android:id="@+id/tv_expense"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="@dimen/_3sdpp"
                                                android:fontFamily="sans-serif-medium"
                                                android:text="0"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                                android:textColor="@color/whiteColor" />

                                        </LinearLayout>

                                    </androidx.cardview.widget.CardView>

                                </LinearLayout>

                            </LinearLayout>

                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/rel"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_10sdpp"
                            android:layout_marginBottom="@dimen/_10sdpp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/field_price_bg"
                                android:gravity="center_horizontal"
                                android:minWidth="@dimen/_70sdpp"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/tv_bookings"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="0"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                    android:textColor="@color/darkTextColor" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_3sdpp"
                                    android:fontFamily="sans-serif"
                                    android:text="@string/bookings"
                                    android:textColor="@color/subTextColor" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdpp"
                                android:background="@drawable/field_price_bg"
                                android:gravity="center_horizontal"
                                android:minWidth="@dimen/_70sdpp"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/tv_hours"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="0"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                    android:textColor="@color/darkTextColor" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_3sdpp"
                                    android:fontFamily="sans-serif"
                                    android:text="@string/hours"
                                    android:textColor="@color/subTextColor" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdpp"
                                android:background="@drawable/field_price_bg"
                                android:gravity="center_horizontal"
                                android:minWidth="@dimen/_70sdpp"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/tv_cancelled"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="0"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                    android:textColor="@color/darkTextColor" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_3sdpp"
                                    android:fontFamily="sans-serif"
                                    android:text="@string/cancelled"
                                    android:textColor="@color/subTextColor" />

                            </LinearLayout>

                        </LinearLayout>

                    </RelativeLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/fac_vu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdpp"
                    android:layout_marginTop="@dimen/_10sdpp"
                    android:layout_marginEnd="@dimen/_15sdpp"
                    app:cardCornerRadius="@dimen/_8sdpp"
                    app:cardElevation="0dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingTop="@dimen/_10sdpp"
                        android:paddingBottom="@dimen/_10sdpp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10sdpp"
                            android:layout_marginEnd="@dimen/_10sdpp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:fontFamily="sans-serif-medium"
                                android:lines="1"
                                android:text="@string/facilities"
                                android:textColor="@color/darkTextColor" />

                            <TextView
                                android:id="@+id/tv_fac_total"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_centerVertical="true"
                                android:fontFamily="sans-serif"
                                android:lines="1"
                                android:text="0 AED"
                                android:textColor="@color/darkTextColor" />

                        </RelativeLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/fac_recycler_vu"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdpp" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recycler_vu"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_10sdpp"
                    android:layout_marginBottom="@dimen/_10sdpp" />

            </LinearLayout>

        </androidx.core.widget.NestedScrollView>

    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

    <FrameLayout
        android:id="@+id/filter_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_below="@id/card_vu"
        android:visibility="gone"
        android:clickable="true"
        android:focusable="true">

        <FrameLayout
            android:id="@+id/filter_container"
            android:layout_width="@dimen/_250sdpp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:clickable="true"
            android:focusable="true" />

    </FrameLayout>

</RelativeLayout>