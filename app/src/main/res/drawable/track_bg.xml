<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/v5grayColor"/>
            <corners android:radius="@dimen/_100sdpp"/>
            <stroke android:width="@dimen/_1sdpp"
                android:color="@color/v5grayColor"/>
            <size android:width="@dimen/_10sdpp"
                android:height="@dimen/_10sdpp"/>
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/v5grayColor"/>
            <corners android:radius="@dimen/_100sdpp"/>
            <stroke android:width="@dimen/_1sdpp"
                android:color="@color/v5grayColor"/>
            <size android:width="@dimen/_10sdpp"
                android:height="@dimen/_10sdpp"/>
        </shape>
    </item>

</selector>